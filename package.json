{
  "name": "todo-matrix",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "pre": "pnpm run lint && pnpm run format:check && pnpm run build"
  },
  "dependencies": {
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10"
}
